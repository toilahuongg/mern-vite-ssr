{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://toilahuong.github.io",
  "scripts": {
    "dev": "tsnd --respawn --project ./tsconfig.server.json ./server/index.ts",
    "build": "rm -rf dist && npm run build:client && npm run build:ssr && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:ssr": "vite build --ssr src/entry.server.tsx --outDir dist/server",
    "build:server": "tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "start": "NODE_ENV=producttion node dist/index.js",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@remix-run/node": "^1.15.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}
