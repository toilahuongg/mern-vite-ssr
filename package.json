{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://toilahuong.github.io",
  "scripts": {
    "dev": "tsnd --respawn --files  -r tsconfig-paths/register --project ./tsconfig.server.json ./server/index.ts",
    "build": "rm -rf dist && npm run build:client && npm run build:ssr && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:ssr": "vite build --ssr src/entry.server.tsx --outDir dist/server",
    "build:server": "tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "start": "NODE_ENV=producttion node dist/index.js",
    "test": "jest",
    "deploy": "gh-pages -d dist"
  },
